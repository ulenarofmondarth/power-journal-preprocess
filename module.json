{
  "id": "mondarth-power-journal",
  "title": "Foundry Journal Enhancements on Steroids",
  "description": "A module for journal enhancement",
  "version": "0.0.0",
  "authors": [
    {
      "name": "Ulenar of Mondarth",
      "url": "https://mondarth.com"
    }
  ],
  "compatibility": {
    "minimum": "12",
    "verified": "12"
  },
  "esmodules": [
    "dist/mondarth-power-journal.js"
  ],
  "flags": {
