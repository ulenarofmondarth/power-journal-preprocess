  "socket": false,
  "url": "https://github.com/Mondarth/EMMA",
  "readme": "https://github.com/Mondarth/EMMA/blob/master/README.md",
  "bugs": "https://github.com/Mondarth/EMMA/issues",
  "changelog": "https://github.com/Mondarth/EMMA/releases/latest/",
  "manifest": "https://github.com/Mondarth/EMMA/releases/latest/download/module.json",
  "download": "https://github.com/Mondarth/EMMA/releases/download/0.0.0/module.zip",
